[
  {
    "name": "toy-api",
    "image": "517691465982.dkr.ecr.eu-west-1.amazonaws.com/toy-api:latest",
    "essential": true,
    "portMappings": [
      {
        "containerPort": 5000,
        "hostPort": 5000
      }
    ],
    "environment": [
      {
        "name": "FLASK_ENV",
        "value": "production"
      },
      {
        "name": "APP_ENV",
        "value": "dev"
      }
    ],
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "/ecs/toy-api",
        "awslogs-region": "eu-west-1",
        "awslogs-stream-prefix": "ecs"
      }
    }
  }
]